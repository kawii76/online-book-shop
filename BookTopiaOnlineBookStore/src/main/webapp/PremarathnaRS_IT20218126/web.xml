<?xml version="1.0" encoding="UTF-8"?>

<element>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  
  <servlet>
		<servlet-name>BookServlet</servlet-name>
		<servlet-class>com.book.BookServlet</servlet-class>
	</servlet>
  
  <servlet-mapping>
		<servlet-name>BookServlet</servlet-name>
		<url-pattern>/BookServlet</url-pattern>
	</servlet-mapping>
	
	
	 <servlet>
		<servlet-name>bookinsert</servlet-name>
		<servlet-class>com.book.BookInsert</servlet-class>
	</servlet>
  
  <servlet-mapping>
		<servlet-name>BookInsert</servlet-name>
		<url-pattern>/BookInsert</url-pattern>
	</servlet-mapping>
	
	
	 <servlet>
		<servlet-name>updatebook</servlet-name>
		<servlet-class>com.employee.UpdateBookServlet</servlet-class>
	</servlet>
  
  <servlet-mapping>
		<servlet-name>UpdateBookServlet</servlet-name>
		<url-pattern>/UpdateBookServlet</url-pattern>
	</servlet-mapping>
	
	 <servlet>
		<servlet-name>deletebook</servlet-name>
		<servlet-class>com.book.DeleteBookServelet</servlet-class>
	</servlet>
  
  <servlet-mapping>
		<servlet-name>DeleteBookServelet</servlet-name>
		<url-pattern>/DeleteBookServelet</url-pattern>
	</servlet-mapping>
	</web-app>
	</element>